{
  "name": "mongoscope",
  "description": "Wouldn't it be great if monogo just had this stuff builtin instead of rolling it yourself?",
  "version": "0.0.3",
  "scripts": {
    "start": "NODE_ENV=development DEBUG=* gulp dev",
    "dist": "./bin/dist.sh",
    "test": "mocha",
    "up": "git pull --rebase && npm install && cd ui && npm install && cd ../ && NODE_ENV=production ./node_modules/.bin/gulp ui;",
    "mongo-cluster": "cat ./test/shell/start-cluster.js | mongo",
    "mongo-replicaset": "mongo ./test/shell/start-replicaset.js",
    "mongo": "mongod --port 27017 --dbpath /data/db/standalone"
  },
  "author": "Lucas Hrabovsky <lucas@mongodb.com> (http://imlucas.com)",
  "homepage": "http://10gen.github.io/mongoscope/onesheet.html",
  "repository": {
    "type": "git",
    "url": "git://github.com/10gen/mongoscope.git"
  },
  "bin": {
    "mongoscope": "./bin/mongoscope.js"
  },
  "lone": {
    "ignore": [
      "gulpfile.js",
      "README.md",
      "aws/**",
      "docs/**",
      "data/**",
      "ui/**",
      "test/**",
      "static/img/*.0*.png",
      "**/mocha/**",
      "**/flash-src/**",
      "**/socket.io-client/components/**",
      "**/socket.io-client/lib/vendor/**",
      "**/node_modules/redis/**",
      "node_modules/**/bin/**",
      "node_modules/**/test?/**",
      "node_modules/**/example?/**",
      "node_modules/**/benchmark?/**",
      "node_modules/**/benchmark.html",
      "node_modules/**/tool?/**",
      "node_modules/gulp?/**",
      "node_modules/**/doc?/**",
      "node_modules/**/*.log",
      "node_modules/**/ext/win32/**",
      "node_modules/**/tmp/**",
      "node_modules/**/*.swf",
      "node_modules/**/*.min.js",
      "node_modules/**/browser_build/**"
    ]
  },
  "dependencies": {
    "express": "^4.0.0",
    "socket.io": "^0.9.16",
    "mongodb": "^1.4.0",
    "debug": "^0.7.4",
    "mongolog": "^0.0.5",
    "diskspace": "^0.1.6",
    "jsonwebtoken": "^0.2.0",
    "nconf": "^0.6.9",
    "request": "^2.34.0",
    "event-stream": "^3.1.1",
    "JSONStream": "^0.8.0",
    "csv-streamify": "^1.0.0",
    "body-parser": "^1.0.1",
    "bson": "^0.2.7",
    "event-source-emitter": "0.0.2",
    "async": "^0.7.0",
    "mongo-query": "^0.5.2",
    "uuid": "^1.4.1",
    "mongodb-ns": "0.0.2",
    "keepup": "0.0.1"
  },
  "devDependencies": {
    "gulp": "^3.5.6",
    "gulp-jade": "^0.4.2",
    "lone": "imlucas/lone",
    "mocha": "^1.17.1",
    "gulp-util": "^2.2.14",
    "supertest": "^0.10.0",
    "lodash": "^2.4.1",
    "jadeify": "^2.1.1",
    "marked": "^0.3.1",
    "ecstatic": "^0.4.13",
    "gulp-uglify": "^0.2.1",
    "gulp-sterno-manifest": "^0.0.1",
    "gulp-less": "^1.2.3",
    "vinyl-source-stream": "^0.1.1",
    "gulp-livereload": "^1.2.0",
    "browserify": "^3.39.0",
    "node-notifier": "^2.0.4",
    "coffee-script": "^1.7.1"
  }
}
