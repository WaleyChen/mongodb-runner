{
  "name": "mongoscope",
  "description": "Wouldn't it be great if monogo just had this stuff builtin instead of rolling it yourself?",
  "version": "0.0.3",
  "scripts": {
    "start": "NODE_ENV=development DEBUG=mongo* gulp dev",
    "dist": "npm install && npm run-script build && rm -rf node_modules/ && npm link lone && npm install --production && NODE_ENV=production DEBUG=* lone",
    "test": "NODE_ENV=testing mocha",
    "build": "echo 'install ui deps and building' && cd ui && npm install && cd ../; gulp ui;"
  },
  "author": "Lucas Hrabovsky <lucas@mongodb.com> (http://imlucas.com)",
  "homepage": "http://10gen.github.io/mongoscope/onesheet.html",
  "repository": {
    "type": "git",
    "url": "git://github.com/10gen/mongoscope.git"
  },
  "bin": {
    "mongoscope": "./bin/mongoscope.js"
  },
  "lone": {
    "ignore": [
      "gulpfile.js",
      "README.md",
      "aws/**",
      "docs/**",
      "data/**",
      "ui/**",
      "test/**",
      "static/img/*.0*.png",
      "**/.bin/**",
      "**/mocha/**",
      "**/flash-src/**",
      "**/socket.io-client/components/**",
      "**/socket.io-client/lib/vendor/**",
      "**/node_modules/redis/**",
      "node_modules/**/bin/**",
      "node_modules/**/test?/**",
      "node_modules/**/example?/**",
      "node_modules/**/benchmark?/**",
      "node_modules/**/benchmark.html",
      "node_modules/**/tool?/**",
      "node_modules/gulp?/**",
      "node_modules/**/doc?/**",
      "node_modules/**/*.log",
      "node_modules/**/ext/win32/**",
      "node_modules/**/tmp/**",
      "node_modules/**/*.swf",
      "node_modules/**/*.min.js",
      "node_modules/**/browser_build/**"
    ]
  },
  "dependencies": {
    "express": "^3.5.1",
    "socket.io": "^0.9.16",
    "mongodb": "^1.4.0",
    "debug": "^0.7.4",
    "mongolog": "^0.0.5",
    "diskspace": "^0.1.6",
    "jsonwebtoken": "^0.2.0",
    "sse-stream": "^0.0.4",
    "up": "git://github.com/learnboost/up",
    "nconf": "^0.6.9",
    "request": "^2.34.0",
    "event-stream": "^3.1.1",
    "JSONStream": "^0.8.0",
    "csv-streamify": "^1.0.0",
  },
  "devDependencies": {
    "gulp": "^3.5.6",
    "gulp-jade": "^0.4.2",
    "lone": "git://github.com/imlucas/lone",
    "mocha": "^1.17.1",
    "gulp-util": "^2.2.14",
    "supertest": "^0.10.0",
    "lone": "imlucas/lone",
    "lodash": "^2.4.1",
    "jadeify": "^2.1.1",
    "marked": "^0.3.1",
    "ecstatic": "^0.4.13",
    "gulp-uglify": "^0.2.1",
    "gulp-sterno-manifest": "^0.0.1",
    "gulp-less": "^1.2.3",
    "vinyl-source-stream": "^0.1.1",
    "gulp-livereload": "^1.2.0",
    "browserify": "^3.39.0",
    "node-notifier": "^2.0.4"
  }
}
