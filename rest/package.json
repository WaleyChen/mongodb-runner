{
  "name": "mongorest",
  "description": "Wouldn't it be great if monogo just had this stuff builtin instead of rolling it yourself?",
  "version": "0.0.1",
  "scripts": {
    "start": "NODE_ENV=development DEBUG=mongorest:* node index",
    "dist": "lone",
    "test": "mocha"
  },
  "dependencies": {
    "express": "~3.4.8",
    "socket.io": "~0.9.16",
    "mongodb": "~1.3.23",
    "debug": "^0.7.4",
    "mongolog": "^0.0.5",
    "lodash": "^2.4.1",
    "diskspace": "^0.1.5",
    "express-jwt": "^0.2.0",
    "jsonwebtoken": "^0.2.0",
    "sse-stream": "0.0.4"
  },
  "devDependencies": {
    "mocha": "^1.17.1"
  },
  "jshintConfig": {
    "eqeqeq": true,
    "node": true,
    "indent": 2,
    "newcap": true,
    "predef": [
      "describe",
      "it",
      "before",
      "beforeEach",
      "after",
      "afterEach"
    ],
    "quotmark": "single",
    "unused": true,
    "trailing": true,
    "boss": true,
    "loopfunc": true
  }
}
